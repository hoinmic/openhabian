//humidifier.url=http://192.168.0.100
//humidifier.updateInterval=1000

// Humidifier
String  humidifier_relayStatus  "Relais Status [%s]"        <poweroutlet>   (gHumidifier)           { http="<[http://192.168.0.100/report:3000:JSONPATH($.relay)]" }
Number  humidifier_power        "Power [%.1f W]"            <poweroutlet>   (gHumidifier, gPower)   { http="<[http://192.168.0.100/report:3000:JSONPATH($.power)]" }
Number  humidifier_temperature  "Temperature [%.1f Â°C]"     <temperature>   (gHumidifier)           { http="<[http://192.168.0.100/temp:3000:JSONPATH($.compensated)]" }
Switch  humidifier_network      "Network Status [%s]"       <poweroutlet>   (gHumidifier, gNetwork) { channel="network:pingdevice:devicename:online" }
Number  humidifier_latency      "Latency [%d ms]"           <poweroutlet>   (gHumidifier, gPing)    { channel="network:pingdevice:devicename:latency" }

Switch  humidifier              "Remote Control [%s]"       <poweroutlet>   (gHumidifier, gSwitch)  { http=">[ON:GET:http://192.168.0.100/relay?state=1] >[OFF:GET:http://192.168.0.100/relay?state=0]" }

Number  battery_HT              "Battery level [%d %%]"     <battery>       (gShellyHT)             { channel="mqtt:topic:mything:battery" } 
Number  humidity_HT             "Humidity [%.1f %%]"        <humidity>      (gShellyHT)             { channel="mqtt:topic:mything:humidity" }
Number  temperature_HT          "Temperature [%.2f %%]"     <temperature>   (gShellyHT)             { channel="mqtt:topic:mything:temperature" }
